  screen syncSimpleView ( ) {
    header ( "Sync Entities" ) {
      backButton ( "back" , onclick = { back(); } )
    }
    var phrase = ""
    searchBox ( phrase )
    list ( ent in Project . searchPrefix ( phrase ) . order ( "id" , true ) ) {
      item {
        checkBox ( ent . sync , label = ent . id )
      }
    }
  }